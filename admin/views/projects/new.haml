.block
  .secondary-navigation
    %ul.wat-cf
      %li.first=link_to pat(:list), url(:projects, :index)
      %li.active=link_to pat(:new), url(:projects, :new)
  .content
    %h2.title
      =pat(:new)
      =mt(:project)
    .inner
      -form_for :project, url(:projects, :create), :class => :form do |f|
        =partial "projects/form", :locals => { :f => f }

-content_for :sidebar, partial("base/sidebar")